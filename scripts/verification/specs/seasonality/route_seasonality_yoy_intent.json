  "expect": {
    "intent": "seasonality",
    "run_trends": true,
    "needs_ids": false
  },
  "intent_goal": {
    "intent_path": "intent",
    "allowed_intents": ["seasonality"]
  },
  "assert": {
    "array_min_len": {
      "customer_ids": 1
    }
  }
}

