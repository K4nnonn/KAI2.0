{
  "id": "ROUTE_SEASONALITY_YOY_INTENT",
  "pillar": "intent",
  "kind": "route",
  "capabilities": ["routing", "seasonality", "account_context"],
  "request": {
    "message": "What months are peak and low demand for loyalty this year vs last year?",
    "session_id": "${SESSION_ID}"
