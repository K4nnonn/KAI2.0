{
  "id": "ROUTE_SEASONALITY_CUSTOM_METRIC_INTENT",
  "pillar": "intent",
  "kind": "route",
  "capabilities": ["routing", "seasonality", "custom_metric_inference"],
  "request": {
    "message": "Is FR_Intent_Clicks seasonal in loyalty, and when are the highest months?",
    "session_id": "${SESSION_ID}"
  },
  "expect": {
    "intent": "seasonality",
    "run_trends": true,
    "needs_ids": false
  },
  "intent_goal": {
    "intent_path": "intent",
    "allowed_intents": ["seasonality"]
  },
  "assert": {
    "array_min_len": {
      "customer_ids": 1
    }
  }
}

